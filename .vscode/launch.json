{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome Remote",
      "port": 9222,
      "url": "http://localhost:8090",
      "webRoot": "${workspaceFolder}/dist",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "ty:///./*": "${workspaceFolder}/*"
      }
    }
  ]
}
